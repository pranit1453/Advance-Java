package com.demo.beans;

import java.time.LocalDate;
import java.util.Set;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.OneToMany;
import javax.persistence.Table;

@Entity
@Table(name = "coursetable")
public class Course {
	@Id
	private int cid;
	private String cname;
	private LocalDate startdate;
	@OneToMany // by default lazy fetch
	private Set<Students> sset;

	public Course() {
		super();
	}

	public Course(int cid, String cname, LocalDate startdate, Students stud) {
		super();
		this.cid = cid;
		this.cname = cname;
		this.startdate = startdate;
		this.stud = stud;
	}

	public int getCid() {
		return cid;
	}

	public void setCid(int cid) {
		this.cid = cid;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public LocalDate getStartdate() {
		return startdate;
	}

	public void setStartdate(LocalDate startdate) {
		this.startdate = startdate;
	}

	public Students getStud() {
		return stud;
	}

	public void setStud(Students stud) {
		this.stud = stud;
	}

	@Override
	public String toString() {
		return "Course [cid=" + cid + ", cname=" + cname + ", startdate=" + startdate + ", stud=" + stud + "]";
	}

}
